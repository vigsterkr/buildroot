-	u32 dwSignature;	/* Used for object validation.   */
-	void *pRefData;		/* Argument for client's ISR.    */
-	ISR_PROC pfnISR;	/* Client's ISR.                 */
-
-	u32 dwIntrID;		/* hardware intertupt identifier */
-};
