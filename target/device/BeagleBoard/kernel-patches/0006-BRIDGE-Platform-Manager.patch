+/*  ----------------------------------- Function Prototypes */
+static DSP_STATUS GetNumOpenChannels(struct CHNL_MGR *hChnlMgr,
+				    OUT u32 *pcOpenChannels);
+
+static DSP_STATUS GetNumChannels(struct CHNL_MGR *hChnlMgr,
+				 OUT u32 *pcChannels);
